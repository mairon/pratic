<h1 align="right" id="header">Documento</h1>
<%= link_to 'Volver', documentos_path %>
<% form_for(@documento) do |f| %>
  <%= f.error_messages %>
  <div class="group">
    <table>
      <tr>
        <td>Nome:</td>
        <td><%= f.text_field :nome %></td>
      </tr>
    </table>
  </div>
  <p>
<%= link_to 'Volver', documentos_path %>  |  <%= f.submit "Grabar" %>
  </p>
<% end %>


